                "Parameters": [
                    { "Name": "NOMECAMPOS", "Description": "Digite o Nome dos Campos separados por virgula: " },
                    { "Name": "NOMECAMPOSDESCRI", "Description": "Digite Descrição dos Campos separados por virgula: " }
